{
  "name": "OTT Dissect",
  "version": "0.2",
  "description": "Dissect the OTT solution",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
     "declarativeContent",
    "webRequest",
	  "webRequestBlocking",
    "cookies",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
      "data/patterns.json"
  ]
}
